#!/usr/bin/env python3
import requests
import re
from html.parser import HTMLParser
#    Youtubeの再生リストから音楽をDLするアプリ
#    youtube_music : データベース
#    名前  URL

user = ""
password = ""
url = "https://www.youtube.com/playlist?list=PL17wzDjTZ76h9kczQuHmVMP5EPE1qAqzE"

pattern = r"/watch?v="
r = requests.get(url)
text = r.text
print(type(text))
tmp = re.match( pattern , text )
print(tmp)
